datasource db {
  // could be postgresql or mysql
  provider = "sqlite"
  url      = "file:dataBase.db"
}

generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model File {
  id           Int     @id @default(autoincrement())
  data         String  @default("")
  nickname     String  @default("")
  expires      BigInt  @default(0)
}
